[user]
	name = nerdondon
	email = willdon@nerdondon.com
[color]
	diff = auto
    status = auto
    branch = auto
    ui = true
[push]
	default = simple
[alias]
	cb = checkout -b
	ci = commit
	co = checkout
	count = !git shortlog -sn
	lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
	pre = pull --rebase
	rmc = rm --cached
	sl = stash list
	st = status
	track = !$ZSH/bin/git-track
